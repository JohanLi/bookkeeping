{
  "manifest_version": 3,
  "name": "bookkeeping",
  "version": "0.0.1",
  "action": { "default_popup": "index.html" },
  "content_scripts": [
    {
      "js": ["src/bank.ts"],
      "matches": [
        "https://*.seb.se/*"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  },
  "permissions": [
    "downloads"
  ]
}
